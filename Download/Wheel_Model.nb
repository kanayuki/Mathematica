Cell["\<\
Here, we can generate a number of solutions, and then display them \
together with a scaled representation of the impulse:\
\>", "Text",
  ImageRegion->{{0, 1}, {0, 1}}],

Cell[BoxData[
    \(gr1 = 
      Plot[Evaluate[
          Flatten[{\(1000\ Pulse[t, 0,  .5]\)\/3000, 
              First[{x1[t], x2[t]} /. \[InvisibleSpace]soln1]}]], {t, 0, 20}, 
        PlotStyle \[Rule] {Hue[0], GrayLevel[0], GrayLevel[0]}, 
        Frame \[Rule] True, PlotRange \[Rule] {0, 1.3}, 
        DisplayFunction \[Rule] Identity]; \)], "Input",
  ImageRegion->{{0, 1}, {0, 1}}],

Cell[BoxData[
    \(soln2 = 
      NDSolve[eqns[1000, 15, 1000, 8000, 1000, 800, 
          2000\ Pulse[t, 0,  .5] + 3000\ Pulse[t, 3,  .6] + 
            1500\ Pulse[t, 5,  .4]], {x1[t], x2[t]}, {t, 0, 20}, 
        MaxSteps \[Rule] 3000]; \)], "Input",
  ImageRegion->{{0, 1}, {0, 1}}],

Cell[BoxData[
    \(gr2 = 
      Plot[Evaluate[
          Flatten[{\(\(1\/3000\)\((2000\ Pulse[t, 0,  .5] + 
                  3000\ Pulse[t, 3,  .6] + 1500\ Pulse[t, 5,  .4])\)\), 
              First[{x1[t], x2[t]} /. \[InvisibleSpace]soln2]}]], {t, 0, 20}, 
        PlotStyle \[Rule] {Hue[0], GrayLevel[0], GrayLevel[0]}, 
        Frame \[Rule] True, PlotRange \[Rule] {0, 1.3}, 
        DisplayFunction \[Rule] Identity]; \)], "Input",
  ImageRegion->{{0, 1}, {0, 1}}],

Cell[CellGroupData[{Cell[BoxData[
    \(Show[GraphicsArray[{{gr1}, {gr2}}], 
      DisplayFunction \[Rule] $DisplayFunction]\)], "Input",
  ImageRegion->{{0, 1}, {0, 1}}],

